{
  "ConnectionStrings": {
    "DefaultConnection": "Host=postgres;Database=anekdotify;Username=anekdotify_user;Password=${POSTGRES_PASSWORD}",
    "cache": "redis:6379,password=${REDIS_PASSWORD}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JWT": {
    "Issuer": "https://anekdotify.at",
    "Audience": "https://anekdotify.at"
  },
  "Frontend": {
    "BaseUrl": "https://anekdotify.at"
  },
  "OAuth": {
    "GitHub": {
      "ClientId": "${GITHUB_CLIENT_ID}",
      "ClientSecret": "${GITHUB_CLIENT_SECRET}"
    },
    "Google": {
      "ClientId": "${GOOGLE_CLIENT_ID}",
      "ClientSecret": "${GOOGLE_CLIENT_SECRET}"
    }
  }
}
